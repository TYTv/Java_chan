<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.student.app_2.MainActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/HomePageTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="HomePage" />

        <Button
            android:id="@+id/ButtonGoPage1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="OnClickGoPage1"
            android:text="Go_Page_1" />

        <Button
            android:id="@+id/ButtonGoPage2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="OnClickGoPage2"
            android:text="Go_Page_2" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
