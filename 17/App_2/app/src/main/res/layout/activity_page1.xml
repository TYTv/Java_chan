<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.student.app_2.ActivityPage1"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/ShowTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Page 1"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="193dp" />

        <Button
            android:id="@+id/ButtonGoHomePage_1"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:onClick="OnClickGoHomePage"
            android:text="Go_Home_Page"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp" />

        <Button
            android:id="@+id/HttpButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="HttpOnClick"
            android:text="http://..." />

        <Button
            android:id="@+id/DialButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="DialOnClick"
            android:text="Dial 0800080090" />

        <Button
            android:id="@+id/CaptureButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="CaptureOnClick"
            android:text="Capture" />

        <Button
            android:id="@+id/MapButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="MapOnClick"
            android:text="Map" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
